interface bond20
 type    ieee8023adLag
 enabled true
 subinterface 0
  ipv4 address 1.1.1.2
   prefix-length 24
  exit
  ipv6 address 1::2
   prefix-length 64
  exit
 exit
!
interface loopback1
 type    softwareLoopback
 enabled true
 subinterface 0
  ipv4 address 100.1.1.2
   prefix-length 32
  exit
  ipv6 address 100:1::1:2
   prefix-length 128
  exit
 exit
!
network-instance default
 table-connection DIRECTLY_CONNECTED ISIS IPV6
 !
 table-connection DIRECTLY_CONNECTED ISIS IPV4
 !
 protocol ISIS default
  global net       [ 49.0002.0102.0203.0405.00 ]
  global level-capability LEVEL_2
  global max-ecmp-paths 16
  global graceful-restart enabled true
  global transport lsp-mtu-size 8974
  global af IPV6 UNICAST
   enabled true
   exit
  !
  global af IPV4 UNICAST
   enabled true
   exit
  !
  level 2
   enabled true
   exit
  !
  interface bond20
   enabled      true
   network-type POINT_TO_POINT
   bfd bfd-tlv true
   af IPV6 UNICAST
    enabled true
   !
   af IPV4 UNICAST
    enabled true
   !
   level 2
    enabled true
    exit
   !
   exit
  !
